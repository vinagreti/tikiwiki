<div class="headings">
	<h2>Headings</h2>
	<div class="row">
		<div class="col-sm-8 col-md-9">
			<div class="h1">h1. Bootstrap heading</div>
		</div>

		<div class="col-sm-4 col-md-3">
			<div class="input">
				<p>
					<label for="sg-h1-font-size">Font size:</label>
					<input id="sg-h1-font-size" class="nocolor" data-selector=".h1" data-element="font-size" data-var="@font-size-h1" type="text">
				</p>
				<p>
					<label for="sg-h1-font-weight">Font weight:</label>
					<input id="sg-h1-font-weight" class="nocolor" data-selector=".h1" data-element="font-weight" type="text">
				</p>
				<p>
					<label for="sg-h1-spacing">Spacing:</label>
					<input id="sg-h1-spacing" class="nocolor" data-selector=".h1" data-element="letter-spacing" type="text">
				</p>
				<p class="picker" data-selector=".h1" data-element="color">
					<label for="sg-h1-color">Color:</label>
					<input id="sg-h1-color" data-selector=".h1" data-element="color" type="text"><span class="input-group-addon"><i></i></span>
				</p>
				<p>
					<input id="sg-h1-uppercase" class="nocolor check" data-selector=".h1" data-element="text-transform" type="checkbox" data-default="none" data-check="uppercase">
					Uppercase
				</p>
			</div>
		</div>
	</div>

